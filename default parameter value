def my_function(superheros="captian america"):
    print(" he is our savior " + superheros)


my_function("haya haya")
my_function("batman")
my_function("thor")
my_function("superman")
my_function("blackman")
# passsing a list as an arguments


def my_function(food):
    for x in food:
        print(x)


fruits = ["apple", "banana", "cherry"]

my_function(fruits)
